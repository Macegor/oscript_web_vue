{
    "Database": {
        "DbType": "SQLite",
        "ConnectionString": "Data Source=mydb.db"
    },
    "Security": {
        "Password": {
            "RequiredLength": 1,
            "RequireNonAlphanumeric": false,
            "RequireLowercase": false,
            "RequireUppercase": false,
            "RequireDigit": false
        },
        "CookieAuth": {
            "LoginPath": "/login",
            "LogoutPath": "/login"
        },
        "Logging": {
            "LogLevel": {
                "Default": "Information"
            }
        }
    }
}